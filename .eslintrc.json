{
    "extends": ["eslint:recommended", "react-app"],
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "semi": "error",
        "semi-spacing": "error",
        "no-var": "error",
        "prefer-const": "error",
        "comma-spacing": "error",
        "no-unused-vars": "error",
        "block-spacing": "error",
        "arrow-spacing": "error",
        "keyword-spacing": "error",
        "rest-spread-spacing": "error",
        "key-spacing": "error",
        "computed-property-spacing": "error"
    }
}
